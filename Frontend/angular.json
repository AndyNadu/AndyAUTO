{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "DealerAutoFrontend": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/dealer-auto-frontend",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "src/app/Pages/home/home.component.css",
              "src/app/Pages/login/login.component.css",
              "src/app/Pages/account/account.component.css",
              "src/app/Pages/listing/listing.component.css",
              "src/app/Pages/buy-a-car/buy-a-car.component.css",
              "src/app/Pages/sell-a-car/sell-a-car.component.css",

              "src/app/Custom Components/car-list/car-list.component.css",
              "src/app/Custom Components/login-form/login-form.component.css",
              "src/app/Custom Components/car-filters/car-filters.component.css",
              "src/app/Custom Components/register-form/register-form.component.css",
              "src/app/Custom Components/sell-a-car-form/sell-a-car-form.component.css",
              "src/app/Custom Components/account-my-cars/account-my-cars.component.css",
              "src/app/Custom Components/account-favourite-cars/account-favourite-cars.component.css",
              "src/app/Custom Components/account-manage-locations/account-manage-locations.component.css",
              "src/app/Custom Components/account-manage-users-list/account-manage-users-list.component.css",
              "src/app/Custom Components/account-personal-information/account-personal-information.component.css",
              "src/app/Custom Components/account-manage-unverified-cars-list/account-manage-unverified-cars-list.component.css",

              "@angular/material/prebuilt-themes/indigo-pink.css",

              "node_modules/bootstrap/dist/css/bootstrap.min.css",

              "primeng/resources/themes/lara-light-blue/theme.css",
              "primeng/resources/primeng.css"
            ],
            "scripts": [
              "node_modules/bootstrap/dist/js/bootstrap.min.js"
            ],
            "server": "src/main.server.ts",
            "prerender": false,
            "ssr": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "DealerAutoFrontend:build:production"
            },
            "development": {
              "buildTarget": "DealerAutoFrontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "DealerAutoFrontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
